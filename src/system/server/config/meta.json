                        {"name": "rna_r9.4.1_70bps_fast.cfg", "custom": false},
                        {"name": "rna_r9.4.1_70bps_fast_prom.cfg", "custom": false},
                        {"name": "rna_r9.4.1_70bps_hac.cfg", "custom": false},
                        {"name": "rna_r9.4.1_70bps_hac_prom.cfg", "custom": false}
                    ],
                    "primers": [
