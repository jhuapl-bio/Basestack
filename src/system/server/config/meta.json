        "basestack_mytax_report": {
            "name": "basestack_mytax_report",
            "title": "MytaxReport",
            "icon": "dna",
            "component": "MyTaxReport",
            "module": true,
            "status": null,
            "objectname": "MyTaxReport",
            "objectfile": "modules/mytax_report",
            "image": "jhuaplbio/basestack_mytax",
            "meta": "${writePath}/basestack_mytax/meta.json",
            "config": {
                "initial": false
            },
            "resources": {
                   "classifiers": [
                        { 
                            "name": "kraken", 
                            "dbs": [
                                { "name":"minikraken", "compressed": true, "included": true },
                                { "name":"flukraken", "compressed":true, "included": true },
                                { "name":"custom", "included": false }
                            ] 
                        },
                        { 
                            "name": "kraken2", 
                            "dbs": [
                                { "name":"minikraken_v1", "compressed": true, "included": false, "source": "ftp://ftp.ccb.jhu.edu/pub/data/kraken2_dbs/old/minikraken2_v1_8GB_201904.tgz", "target": "${writePath}/basestack_mytax/minkraken2" },
                                { "name":"custom", "included": false }
                            ] 
                        },
                        { 
                            "name": "centrifuge", 
                            "dbs": [
                                { "name":"minicentrifuge", "compressed":false, "included": false, "source": "https://genome-idx.s3.amazonaws.com/centrifuge/p_compressed%2Bh%2Bv.tar.gz", "target": "${writePath}/basestack_mytax/minicentrifuge"   },
                                { "name":"custom", "included": false }
                            ] 
                        }                          
                    ]
            },
            "pipelines": [
                {
                    "key": "basestack_mytax_report",
                    "status": null,
                    "title": "Create Report and JSON",
                    "cmd": [
                        "bash", "-c",
                        "source /opt/conda/etc/profile.d/conda.sh",
			            "&& conda activate mytax && kraken --db /opt/databases/${db.name} --output /opt/data/${data.name}.out /opt/data/${data.name}",
			            "&& kraken-report --db /opt/databases/${db.name}  /opt/data/${data.name}.out | tee  /opt/data/${data.name}.report",
			            "&& bash krakenreport_fullstring.sh -i /opt/data/${data.name}.report -k /opt/databases/${db.name} -o /opt/data/${data.name}.fullstring",
			            "&& bash krakenreport2json.sh -i /opt/data/${data.name}.fullstring -o /opt/data/${data.name}.json"
                    ],
                    "dependencies": [
                        {
                            "key": "data",
                            "type": "file",
                            "watch": "true",
                            "statuses": [
                                {
                                    "key": "out",
                                    "title": "Outfile",
                                    "step": 1,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}.out"
                                },
                                {
                                    "key": "report",
                                    "title": "Report",
                                    "step": 2,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}.report"
                                },
                                {
                                    "key": "fullstring",
                                    "title": "String File",
                                    "step": 3,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}.fullstring"
                                },
                                {
                                    "key": "json",
                                    "title": "Final JSON",
                                    "step": 4,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}.json"
                                }
                            ]
                        }
                    ]
                },
                {
                    "key": "basestack_mytax_build_flukraken",
                    "status": null,
                    "title": "Build Standard Flukraken",
                    "cmd": [ "bash", 
                        "-c", 
                        "source /opt/conda/etc/profile.d/conda.sh && conda activate mytax && bash build_flukraken.sh -k /opt/databases/"
                    ],
                    "dependencies": [
                        {
                            "key": "db",
                            "type": "dir",
                            "watch": "true",
                            "statuses": [
                                {
                                    "key": "references",
                                    "title": "References",
                                    "step": 1,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/raw/influenza.fna"
                                },
                                {
                                    "key": "tax",
                                    "title": "Taxonomy",
                                    "step": 2,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/taxonomy/names.dmp"
                                },
                                {
                                    "key": "metadata_table",
                                    "title": "Metadata",
                                    "step": 3,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/raw/annotation_IVR.dat"
                                },
                                {
                                    "key": "final",
                                    "title": "Final",
                                    "step": 4,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/database.kdb"
                                }
                            ]
                        }
                    ]
                },
                {
                    "key": "basestack_mytax_process_db",
                    "status": null,
                    "title": "Pre-Process Kraken Database",
                    "cmd": [
                        "bash", "-c", "source /opt/conda/etc/profile.d/conda.sh && conda activate mytax && bash process_krakendb.sh -k /opt/databases"
                    ],
                    "dependencies": [
                        {
                            "key": "db",
                            "type": "dir",
                            "watch": "true",
                            "statuses": [
                                {
                                    "key": "kdb",
                                    "title": "KDB File",
                                    "step": 1,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/database.kdb"
                                },
                                {
                                    "key": "names",
                                    "title": "Names.dmp",
                                    "step": 2,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/taxonomy/names.dmp"
                                },
                                {
                                    "key": "nodes",
                                    "title": "Nodes.dmp",
                                    "step": 3,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/taxonomy/nodes.dmp"
                                },
                                {
                                    "key": "idx",
                                    "title": "Idx (Index)",
                                    "step": 4,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/database.idx"
                                },
                                {
                                    "key": "joined_full",
                                    "title": "Joined.full",
                                    "step": 5,
                                    "status": null,
                                    "statusType": "file",
                                    "filepath": "${self.path}/taxonomy/joined.full"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
