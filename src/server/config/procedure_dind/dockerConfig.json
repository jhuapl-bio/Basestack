{ 
    "name": "basestack_dind",
    "ExposedPorts": {
    },
    "Privileged": true,
    "HostConfig": {
        "AutoRemove": false,
        "Privileged": true,
        "Binds": [
            "basestack-docker:/var/lib/docker",
            "basestack-docker-certs-ca:/certs/ca",
            "basestack-docker-certs-client:/certs/client"
        ],	 
        "PortBindings": {
        }
    },
    "Volumes": {
    },  
    "Env": [
        "DOCKER_TLS_CERTDIR=/certs"
    ], 
    "NetworkingConfig": {
        "EndpointsConfig": {
            "basestack-network": {
                "Aliases": ["docker"]
            }
        }
    },
    "Tty": true,
    "Image": "jhuaplbio/basestack_orchestrator",
    "Cmd": []
}