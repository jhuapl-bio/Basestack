{
    "compilerOptions": {
        "target": "esnext",
        "useDefineForClassFields": true,
        "module": "esnext",
        "moduleResolution": "node",
        "strict": false,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "types": [ 
            "vite/client"
        ]
    },
    "include": [
        "./**/*.ts",
        "./**/*.d.ts",
        "./**/*.tsx",
        "./**/*.vue"
    ],
    "noImplicitAny": false,
}