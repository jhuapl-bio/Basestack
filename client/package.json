{
  "name": "Basestack",
  "version": "0.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Merritt-Brian/Basestack"
  },
  "author": "Brian Merritt <Brian.Merritt@jhuapl.edu>",
  "description": ": Basestack analyzes and generates reports from Nanopore-based consensus",
  "license": "GPLv3.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:linux": "node .electron-vue/build.js && electron-builder --linux AppImage",
    "build:mac-zip": "node .electron-vue/build.js && electron-builder --mac  zip",
    "package:mac-zip": "electron-builder --mac zip",
    "package:mac": "electron-builder --mac",
    "package:mac-pkg": "electron-builder --mac pkg",
    "package:mac-dmg": "electron-builder --mac dmg",
    "build:mac-dmg": "node .electron-vue/build.js && electron-builder --mac  dmg",
    "build:mac": "node .electron-vue/build.js && electron-builder --mac",
    "build:win": "node .electron-vue/build.js && electron-builder --win",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "publish:linux": "node .electron-vue/build.js && electron-builder --linux AppImage -p always",
    "publishOnly:linux": "electron-builder --linux AppImage -p always",
    "publish:mac-dmg": "node .electron-vue/build.js && electron-builder --mac  dmg -p always",
    "publish:mac": "node .electron-vue/build.js && electron-builder -p always",
    "publishOnly:mac-dmg": "electron-builder --mac  dmg -p always",
    "publishOnly:mac-zip": "electron-builder --mac zip -p always",
    "publishOnly:mac": "electron-builder --mac -p always",
    "publish:win": "node .electron-vue/build.js && electron-builder --win -p always",
    "publishOnly:win": "electron-builder --win -p always",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Basestack",
    "appId": "Basestack App",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data",
        "filter": [
          "config/**/*"
        ]
      }
    ],
    "afterSign": "notarize.js",
    "dmg": {
      "icon": "build/icons/icon.icns",
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": "default",
      "hardenedRuntime" : true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlement.mac.plist",
      "entitlementsInherit": "build/entitlement.mac.plist"
    },
    "win": {
      "icon": "\"build/icons/icon_256x256.png\""
    },
    "linux": {
      "icon": "build/icons/icon_256x256.png"
    },
    "nsis": {
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "allowElevation": true,
      "oneClick": false
    },
    "publish": {
      "provider": "github",
      "releaseType": "release"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.18.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.10.1",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "dockerode": "^3.2.0",
    "electron-log": "^4.2.4",
    "electron-promise-ipc": "^2.1.0",
    "electron-updater": "^4.3.5",
    "epic-spinners": "^1.1.0",
    "express": "^4.17.1",
    "jquery": "^3.5.0",
    "js-cookie": "^2.2.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.25.3",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "sweetalert2": "^9.10.9",
    "systeminformation": "^4.31.1",
    "v-tooltip": "^2.0.3",
    "vue": "^2.5.16",
    "vue-carousel": "^0.18.0",
    "vue-electron": "^1.0.6",
    "vue-js-toggle-button": "^1.3.3",
    "vue-moment": "^4.1.0",
    "vue-multiselect": "^2.1.6",
    "vue-pdf": "^4.1.0",
    "vue-router": "^3.0.1",
    "vue-scroll-snap": "^0.1.10",
    "vue-scrollto": "^2.18.2",
    "vue-sweetalert2": "^3.0.3",
    "vue2-dropzone": "^3.6.0",
    "vuedraggable": "^2.24.1",
    "vuelidate": "^0.7.5",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.8.6",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^9.3.0",
    "electron-builder": "^22.8.1",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
